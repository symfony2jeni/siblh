{% extends 'siblhmantenimientoBundle::standard_layout.html.twig' %}

{% block body -%}
    <h1>BlhDonacion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fechadonacion</th>
                <td>{{ entity.fechaDonacion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Responsabledonacion</th>
                <td>{{ entity.responsableDonacion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('blhdonacion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('blhdonacion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
