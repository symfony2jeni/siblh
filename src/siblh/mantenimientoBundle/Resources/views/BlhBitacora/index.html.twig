{% extends '::base.html.twig' %}

{% block body -%}
    <h1>BlhBitacora list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fechaaccion</th>
                <th>Codigo</th>
                <th>Tabla</th>
                <th>Usuario</th>
                <th>Accion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('blhbitacora_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{% if entity.fechaAccion %}{{ entity.fechaAccion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.codigo }}</td>
                <td>{{ entity.tabla }}</td>
                <td>{{ entity.usuario }}</td>
                <td>{{ entity.accion }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('blhbitacora_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('blhbitacora_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('blhbitacora_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
