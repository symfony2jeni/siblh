
{% extends ajax ? "siblhmantenimientoBundle::plantilla.html_ajax.twig" :
                    "siblhmantenimientoBundle::plantilla.html.twig" %}
{% block body -%}
    <h1>Solicitud</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Codigosolicitud</th>
                <td>{{ entity.codigoSolicitud }}</td>
            </tr>
            <tr>
                <th>Volumenpordia</th>
                <td>{{ entity.volumenPorDia }}</td>
            </tr>
            <tr>
                <th>Acideznecesaria</th>
                <td>{{ entity.acidezNecesaria }}</td>
            </tr>
            <tr>
                <th>Caloriasnecesarias</th>
                <td>{{ entity.caloriasNecesarias }}</td>
            </tr>
            <tr>
                <th>Pesodia</th>
                <td>{{ entity.pesoDia }}</td>
            </tr>
            <tr>
                <th>Volumenportoma</th>
                <td>{{ entity.volumenPorToma }}</td>
            </tr>
            <tr>
                <th>Tomapordia</th>
                <td>{{ entity.tomaPorDia }}</td>
            </tr>
            <tr>
                <th>Fechasolicitud</th>
                <td>{{ entity.fechaSolicitud|date('d-m-Y') }}</td>
            </tr>
            <tr>
                <th>Cuna</th>
                <td>{{ entity.cuna }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Responsable</th>
                <td>{{ entity.responsable }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('blhsolicitud') }}">
            Regresar a Lista de Solicitudes
        </a>
    </li>
    <li>
        <a href="{{ path('blhsolicitud_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    {#</li>
    <li>{{ form(delete_form) }}</li>
</ul>#}
{% endblock %}
