        {{ form_label(form.horaInicioCurva, 'Hora inicio')}}
               {{ form_errors(form.horaInicioCurva)}}
        {{ form_widget(form.horaInicioCurva) }} <br></td>
         </tr>
   
   </tr>
   
    
    <tr>          
      <td>
 <P align="center"><font color="red">*</font>  {{ form_label(form.tiempo1, 'Tiempo1 ' )}}</P></td>
