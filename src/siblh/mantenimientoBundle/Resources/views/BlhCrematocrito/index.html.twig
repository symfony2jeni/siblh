                <td>
                <ul>
                    <li>
                        <a href="{{ path('blhcrematocrito_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('blhcrematocrito_edit', { 'id': entity.id }) }}">edit</a>
