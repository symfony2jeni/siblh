                <td>{{ entity.codigoAnalisisFisicoQuimico }}</td>
                <td>{{ entity.crema1 }}</td>
                <td>{{ entity.crema2 }}</td>
                <td>{{ entity.crema3 }}</td>
                <td>{{ entity.ct1 }}</td>
                <td>{{ entity.ct2 }}</td>
                <td>{{ entity.ct3 }}</td>
