    <h1>Pasteurizacion</h1>

    <table  id = "left" border = "1"  align = "center" bordercolor="#2e6e9e">
        <tbody>
                <tr align = "left"> 
                <th>Codigo de Pasteurizacion</th>
                <td>{{ entity.codigoPasteurizacion }}</td></tr>
                <tr align = "left"> 
                <th>Numero de Ciclo</th>
                <td>{{ entity.numCiclo }}</td></tr>
                <tr align = "left"> 
                <th>Volumen Pasteurizado</th>
                <td>{{ entity.volumenPasteurizado }}</td></tr>
                <tr align = "left"> 
                <th>Cantidad de Frascos</th>
                <td>{{ entity.numFrascosPasteurizados }}</td></tr>
                <tr align = "left"> 
                <th>Fecha Pasteurizacion</th>
                <td>{{ entity.fechaPasteurizacion|date('Y-m-d') }}</td></tr>
                <tr align = "left"> 
                <th>Hora Inicio Pasteurizacion</th>
                <td>{{ entity.horaInicioP|date('H:i:s') }}</td></tr>
                <tr align = "left"> 
                <th>Hora Final Pasteurizacion</th>
                <td>{{ entity.horaFinalP|date('H:i:s') }}</td></tr>
                <tr align = "left"> 
                <th>Hora Inicio Enfriamiento</th>
                <td>{{ entity.horaInicioE|date('H:i:s') }}</td></tr>
                <tr align = "left"> 
                <th>Hora Final Enfriamiento</th>
                <td>{{ entity.horaFinalE|date('H:i:s') }}</td></tr>
                <tr align = "left"> 
                <th>Responsable</th>
                <td>{{ entity.responsablePasteurizacion }}</td></tr>
        </tbody>
    </table>

