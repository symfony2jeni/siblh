{% extends 'siblhmantenimientoBundle::plantilla.html.twig' %} 

{% block stylesheets %}
    {{ parent() }}
        <link href="{{ asset('bundles/siblhmantenimiento/css/form.css') }}" rel="stylesheet" type="text/css" />
 {% endblock %}
        
 {% block javascripts %}
    {{parent()}}
    
    <script type="text/javascript" src="{{ asset('bundles/siblhmantenimiento/js/BlhTemperaturaPasteurizacion/New.js') }}" ></script>
    
 {% endblock %}
        


{% block body -%}
        
    <h1>Editar Temperatura</h1>
    
    {{ form_start(edit_form) }}
    {{ form_errors(edit_form) }}
    
<table align = "center" >
          
 <table align = "center" > 
  <tr><td colspan=2 align = "left" height=10  ></td></tr>
 <tr><td align='left' ><font color="red">*</font>
  {{ form_label(edit_form.temperaturaP,'Temperatura') }}
 </td><td align='left'>{{ form_widget(edit_form.temperaturaP,{ 'attr': {'title': 'Temperatura entre 62 y 66 grados','size': '5','onKeyPress':"return soloNumerosEnteros(event);"} }) }}</td></tr>

 <tr style="visibility:hidden">
  <td align='left' ><font color="red">*</font>        
  {{ form_label(edit_form.idPasteurizacion,'Pasteurizacion') }}
 </td><td align='left'>{{ form_widget(edit_form.idPasteurizacion) }}</td></tr> 
 
 <tr><td colspan = '2' align = 'center'>  <input id= "boton1" type="submit" value="Actualizar"/>
 <input id = "boton" type="button" value="Regresar" onClick=' window.location.href="{{ path('blhtemperaturapasteurizacion') }}" '></td></tr>

          </table>
    {{ form_end(edit_form) }}

{% endblock %}       
 