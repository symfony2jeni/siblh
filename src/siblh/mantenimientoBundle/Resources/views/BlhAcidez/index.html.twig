{% extends '::base.html.twig' %}

{% block body -%}
    <h1>BlhAcidez list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Acidez1</th>
                <th>Acidez2</th>
                <th>Acidez3</th>
                <th>Factor</th>
                <th>Resultado</th>
                <th>Mediaacidez</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('blhacidez_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.acidez1 }}</td>
                <td>{{ entity.acidez2 }}</td>
                <td>{{ entity.acidez3 }}</td>
                <td>{{ entity.factor }}</td>
                <td>{{ entity.resultado }}</td>
                <td>{{ entity.mediaAcidez }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('blhacidez_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('blhacidez_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('blhacidez_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
