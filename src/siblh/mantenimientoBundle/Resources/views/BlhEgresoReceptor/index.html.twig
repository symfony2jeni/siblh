{% extends 'siblhmantenimientoBundle::plantilla.html.twig' %}

{% block javascripts %}
    {{parent()}}
   <script type="text/javascript" src="{{ asset('bundles/siblhmantenimiento/js/BlhEgresoReceptor/index.js') }}" ></script>
{% endblock %}
   
{% block body -%}
 <link href="{{asset('bundles/jquery/jquery.jqGrid-4.5.4/css/ui.jqgrid.css')}}" rel="stylesheet"  type="text/css" media="all"/>  
 
 <script type="text/javascript" src="{{asset('bundles/jquery/jquery.jqGrid-4.5.4/js/i18n/grid.locale-es.js')}}"></script>
      <script type="text/javascript" src="{{asset('bundles/jquery/jquery.jqGrid-4.5.4/js/jquery.jqGrid.min.js')}}" ></script> 

 
    <h1>Egreso Receptor</h1>
<table id = "listado_Egresos">
        <thead>
            <tr>
                <th>Codigo</th>
                <th>Nombre</th>                
                <th>Madre&nbsp;canguro</th>
                <th>Tipo&nbsp;de&nbsp;egreso</th>
                <th>Traslado&nbsp;periferico</th>
                <th>Permanencia&nbsp;ucin</th>
                <th>Fecha&nbsp;de&nbsp;egreso</th>
                <th>Estancia&nbsp;hospitalaria</th>
                <th>Accion</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in egreso_receptores %}
            <tr>
                <td>{{ entity.codigoReceptor }}</td>
                <td>{{ entity.nombre1 }} {{ entity.nombre2 }} {{ entity.nombre3 }} {{ entity.apellido1 }} {{ entity.apellido2 }}</td>
                <td>{{ entity.madreCanguro }}</td>
                <td>{{ entity.tipoEgreso }}</td>
                <td>{{ entity.trasladoPeriferico }}</td>
                <td>{{ entity.permanenciaUcin }}</td>
                <td>{% if entity.fechaEgreso %}{{ entity.fechaEgreso|date('Y-m-d') }}{% endif %}</td>
                <td>{{ entity.estanciaHospitalaria }}</td>
                <td><a href="{{ path('blhegresoreceptor_edit', { 'id': entity.id }) }}"><font color="blue"><u>Editar</u></font></a></td>
                   
            </tr>
        {% endfor %}
        </tbody>
    </table>
<div id='pagerEgresos' ></div>
    {% endblock %}