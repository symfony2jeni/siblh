{% extends 'siblhmantenimientoBundle::plantilla.html.twig'%}

{% block body -%}
    <h1>BlhInformacionPublica list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Documento</th>
                <th>Tipo</th>
                <th>Nombredocumento</th>
                <th>Fechapublicacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('blhinformacionpublica_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.path }}</td>
                <td>{{ entity.tipo }}</td>
                <td>{{ entity.nombreDocumento }}</td>
                <td>{% if entity.fechaPublicacion %}{{ entity.fechaPublicacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('blhinformacionpublica_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('blhinformacionpublica_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

      {#   <ul>
        <li>
            <a href="{{ path('blhinformacionpublica_new') }}">
                Create a new entry
            </a>
        </li>
    </ul> #}
    {% endblock %}